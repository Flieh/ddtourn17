<table>
  <tbody>
    <th>player</th>
    <th>board</th>
    <th>score</th>
    <th>country</th>
    {{#each test}}
    <tr>
      <td class="right-padding">{{{this.name}}}</td>
      <td class="right-padding">{{{this.topBoard}}}</td>
      <td class="right-padding">{{{this.topBoardScore}}}</td>
      <td class="right-padding">{{{this.topBoardCountry}}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>